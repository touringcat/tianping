#navgation-bar li.shadowstart:after{
	content: '';
	background-color: #e0e0e0;
	width: 14px;
	height: 15px;
	border-radius: 14px;
	position: absolute;
	display: block;
	left:-2px;
	top:-2px;
	z-index: -1;
	opacity: 1;
	box-shadow: inset 0px 1px 1px #cccccc;
	box-shadow: inset 0px 1px 1px #cccccc;
}

#navgation-bar li.shadowfinish:after{
	content: '';
	background-color: #4fc1e9;
	width: 14px;
	height: 14px;
	border-radius: 14px;
	position: absolute;
	display: block;
	left:-2px;
	top:-2px;
	z-index: -1;
	opacity: 1;
}

#navgation-bar li.current span{
	background-color: #ffffff;
	width: 16px;
	height: 16px;
	margin-top: -3px;
	margin-left: -3px;
	border: 1px #dddddd solid;
	box-shadow: 0px 1px 1px rgba(0,0,0,.2);
}

#navgation-bar .current span{
	opacity: 1 !important;
}

#navgation-bar li.current span{
	background-color: #ffffff;
	width: 16px;
	height: 16px;
	margin-top: -3px;
	margin-left: -3px;
	border: 1px #dddddd solid;
	box-shadow: 0px 1px 1px rgba(0,0,0,.2);
}

#navgation-bar li .des{
	position: absolute;
	left: -8px;
	top: -30px;
	padding: 0px 6px;
	background-color: #e0e1e6;
	border-radius: 3px;
	box-shadow: inset 0px 1px 1px #cccccc;
}

#navgation-bar li .des:after{
	content: "";
	position: absolute;
	bottom: -10px;
	left: 8px;
	display: block;
	border-style:solid;
	border-color:#e0e1e6 rgba(0,0,0,0) rgba(0,0,0,0) rgba(0,0,0,0); 
	height:0; 
	width:0; 
	border-width: 5px;
}

#navgation-bar li .des p{
	margin-bottom: 0;
	padding: 0;
	font-size:12px;
    	color: white;
}

#navgation-bar li.finished .des,
#navgation-bar li.current .des{
	background-color: #4fc1e9;
	box-shadow: none;
}
#navgation-bar li.finished .des:after,
#navgation-bar li.current .des:after{
	border-top-color: #4fc1e9;
}


